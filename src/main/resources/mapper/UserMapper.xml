<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chat.chatpro.mapper.UserMapper">


	<insert id="insert" parameterType="User" useGeneratedKeys="true" keyProperty="userid">
		insert into user_info (username,password,create_Time) values(#{username},#{password},
		#{createTime})
	</insert>

</mapper>
